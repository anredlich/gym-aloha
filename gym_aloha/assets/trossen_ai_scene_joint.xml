    <geom name="left_wall_bottom" pos="-0.8 0 -0.55" size="0.5 0.4 0.01" type="plane" quat="0.5 0.5 0.5 0.5" material="left_wall" class="backdrop"/> 
    <geom name="left_wall_top" pos="-0.8 0 0.45" size="0.5 0.6 0.01" type="plane" quat="0.5 0.5 0.5 0.5" material="left_wall" class="backdrop"/>
    <!-- Right side wall -->
    <geom name="right_wall_bottom" pos="0.8 0 -0.55" size="0.5 0.4 0.01" type="plane" quat="0.5 -0.5 -0.5 0.5" material="right_wall" class="backdrop"/>  
    <geom name="right_wall_top" pos="0.8 0 0.45" size="0.5 0.6 0.01" type="plane" quat="0.5 -0.5 -0.5 0.5" material="right_wall" class="backdrop"/>  
    
    <geom name="floor" size="20 20 0.05" type="plane" material="carpet" pos="0 0 -.75"/>
    <site name="worldref" pos="0 0 -0.75"/>
 
